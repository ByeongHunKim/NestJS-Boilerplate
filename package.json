{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "local:start": "NODE_ENV=local nest start",
    "local:start:watch": "npm run local:start -- --watch",
    "local:check": "npm run local:test && npm run local:lint && npm run local:build",
    "local:test": "NODE_ENV=local jest",
    "local:lint": "eslint \"{config,src,test}/**/*.ts\"",
    "local:lint:fix": "npm run local:lint -- --fix",
    "local:build": "NODE_ENV=local nest build",
    "local:db:generate-client": "dotenv -e .env -e .env.local prisma generate",
    "local:db:migrate": "dotenv -e .env -e .env.local prisma migrate deploy",
    "local:db:dev": "dotenv -e .env -e .env.local prisma migrate dev",
    "local:db:push": "dotenv -e .env -e .env.local prisma db push",
    "local:db:seed": "dotenv -e .env -e .env.local ts-node prisma/seed.ts",
    "dev:test": "NODE_ENV=development jest --no-colors",
    "dev:build": "NODE_ENV=development nest build",
    "dev:start": "NODE_ENV=development pm2 reload bookpot-api",
    "dev:start:firstTime": "NODE_ENV=development pm2 start pm2.config.js",
    "dev:db:migrate": "dotenv -e .env -e .env.development prisma migrate deploy",
    "dev:db:generate-client": "dotenv -e .env -e .env.development prisma generate",
    "prod:test": "NODE_ENV=production jest --no-colors",
    "prod:build": "NODE_ENV=production nest build",
    "prod:start": "NODE_ENV=production pm2 reload bookpot-api",
    "prod:start:firstTime": "NODE_ENV=production pm2 start pm2.config.js",
    "prod:db:migrate": "dotenv -e .env -e .env.production prisma migrate deploy",
    "prod:db:generate-client": "dotenv -e .env -e .env.production prisma generate"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "4.0.0",
    "@nestjs/swagger": "7.1.6",
    "@nestjs/terminus": "10.1.1",
    "@prisma/client": "5.0.0",
    "axios": "1.4.0",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "passport": "0.6.0",
    "passport-google-oauth20": "2.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "tiny-invariant": "1.3.1",
    "ts-custom-error": "3.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/multer": "1.4.7",
    "@types/node": "18.15.11",
    "@types/passport-google-oauth20": "2.0.11",
    "@types/passport-kakao": "1.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "7.2.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "jest-mock-extended": "3.0.5",
    "prettier": "^2.3.2",
    "prisma": "5.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
